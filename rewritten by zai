local Esp = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/x114/RobloxScripts/main/OpenSourceEsp"))()
-- Boxes --
Esp.Box = true
Esp.BoxColor = Color3.fromRGB(255,255,255)
Esp.BoxOutline = true
Esp.BoxOutlineColor = Color3.fromRGB(0,0,0)
-- HealthBars --
Esp.HealthBar = true
Esp.HealthBarSide = "Left" -- Left,Bottom,Right
-- Names --
Esp.Names = true
Esp.NamesColor = Color3.fromRGB(255,255,255)
Esp.NamesOutline = true
Esp.NamesFont = 2
Esp.NamesSize = 15


plr = game.Players.LocalPlayer
hum = plr.Character.HumanoidRootPart
mouse = plr:GetMouse()

mouse.KeyDown:connect(function(key)
if key == "`" then
if mouse.Target then
hum.CFrame = CFrame.new(mouse.Hit.x, mouse.Hit.y + 3, mouse.Hit.z)
end
end
end)


local UserInputService = game:GetService("UserInputService")
local camera = game.Workspace.CurrentCamera
local fov = camera.FieldOfView
local keyzoom = Enum.KeyCode.G
local zoomfuck = false

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == keyzoom then
        zoomfuck = true
        while zoomfuck do
            camera.FieldOfView = 15
            wait()
        end
    end
end)

UserInputService.InputEnded:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == keyzoom then
        zoomfuck = false
        camera.FieldOfView = 120
    end
end)



loadstring(game:HttpGet("https://raw.githubusercontent.com/HannahxZxy/DUMP/main/pdgunmods"))()

